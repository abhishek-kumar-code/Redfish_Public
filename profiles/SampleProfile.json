{
	"@odata.type": "RedfishProfile.v1_0_0.RedfishProfile",
	"ProfileName": "Anchovy",
	"Version": "1.0.2",
	"Author": "Pizza Box Project",
	"Purpose": "This is a sample Redfish Interoperability profile.",
	"ContactInfo": "pizza@contoso.com",
	"RequiredProfiles": {
		"DMTFBasic": {
			"MinVersion": "1.0.0"
		},
		"ContosoPizza": {
			"OwningEntity": "Other",
			"OwningEntityName": "Contoso",
			"Repository": "contoso.com/profiles",
			"MinVersion": "1.0.0"
		}
	},
	"Protocol": {
		"MinVersion": "1.2",
		"DiscoveryRequired": true
	},
	"Resources": {
		"ChassisCollection": {
			"Members": {
				"MinCount": 1
			}
		},
		"ComputerSystemCollection": {
			"Members": {
				"MinCount": 1
			}
		},
		"ComputerSystem": {
			"MinVersion": "1.1.0",
			"Purpose": "Every implementation must have the logical view of the System.",
			"PropertyRequirements": {
				"SerialNumber": {
					"Requirement": "Mandatory"
				},
				"Manufacturer": {},
				"Model": {
					"Requirement": "Recommended"
				},
				"IndicatorLED": {
					"Requirement": "Recommended",
					"ConditionalRequirements": [{
						"Purpose": "Physical and composed Systems must have a writable Indicator LED",
						"CompareProperty": "SystemType",
						"Comparison": "AnyOf",
						"CompareValues": ["Physical", "Composed"],
						"Requirement": "Mandatory",
						"Writable": true
					}]
				}
			},
			"ActionRequirements": {
				"Reset": {
					"Requirement": "Mandatory",
					"Purpose": "Ability to reset the system is a core requirement of most users.",
					"Parameters": {
						"ResetType": {
							"MinSupportValues": ["ForceOff", "PowerCycle"],
							"Requirement": "Mandatory"
						}
					}
				}
			}
		},
		"EthernetInterface": {
			"MinVersion": "1.2.0",
			"Requirement": "Recommended",
			"ConditionalRequirements": [{
				"SubordinateToResource": ["Manager", "EthernetInterfaceCollection"],
				"Requirement": "Mandatory"
			}],
			"PropertyRequirements": {
				"MACAddress": {
					"Purpose": "MAC address is frequently used to identify systems via DHCP tables or other network sources."
				},
				"Status": {
					"PropertyRequirements": {
						"Health": {},
						"State": {}
					}
				},
				"HostName": {
					"Requirement": "Recommended",
					"ConditionalRequirements": [{
						"SubordinateToResource": ["ComputerSystem", "EthernetInterfaceCollection"],
						"Requirement": "Mandatory"
					}]
				},
				"IPv6Addresses": {
					"Requirement": "Mandatory",
					"MinCount": 1,
					"ConditionalRequirements": [{
						"SubordinateToResource": ["ComputerSystem", "EthernetInterfaceCollection"],
						"MinCount": 2
					}]
				}
			}
		},
		"ManagerCollection": {
			"Members": {
				"MinCount": 1
			}
		},
		"Thermal": {
			"MinVersion": "1.1.0",
			"PropertyRequirements": {
				"Fans": {
					"Requirement": "IfImplemented",
					"PropertyRequirements": {
						"Reading": {},
						"ReadingUnits": {},
						"Status": {},
						"PhysicalContext": {}
					}
				},
				"Temperatures": {
					"Requirement": "Mandatory",
					"MinCount": 2,
					"PropertyRequirements": {
						"ReadingCelsius": {},
						"PhysicalContext": {
							"Comparison": "AllOf",
							"Values": ["CPU", "Ambient"]
						},
						"UpperThresholdFatal": {
							"Requirement": "Recommended"
						},
						"UpperThresholdCritical": {},
						"UpperThresholdNonCritical": {}
					}
				}
			}
		},
		"ContosoTimeMachine": {
			"OwningEntity": "Other",
			"OwningEntityName": "Contoso",
			"Repository": "www.contoso.com/schemas",
			"PropertyRequirements": {
				"CurrentTime": {},
				"DestinationTime": {},
				"IsGrandfatherAlive": {
					"Requirement": "Recommended"
					},
				"ParadoxDetected": {
					"Requirement": "IfImplemented"
					}
			}
		}
	},
	"Registries": {
		"Base": {
			"MinVersion": "1.0.0",
			"Repository": "redfish.dmtf.org/registries",
			"Messages": {
				"Success": {},
				"GeneralError": {},
				"Created": {},
				"PropertyDuplicate": {}
			}
		},
		"ContosoPizzaMessages": {
			"OwningEntity": "Other",
			"OwningEntityName": "Contoso",
			"Repository": "contoso.com/registries",
			"Requirement": "Mandatory"
		}
	}
}
