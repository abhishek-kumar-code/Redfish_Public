          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/enumeration"/>
          <Annotation Term="Redfish.Yang.if_feature" String="if-mib"/>
          <Annotation Term="Redfish.Yang.mandatory" EnumMember="Redfish.Yang.Mandatory/true"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB - ifAdminStatus"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The desired state of the interface. This leaf has the same read semantics as ifAdminStatus."/>
         </Property>
        <Property Name="oper_status" Type="ietf_interfaces.interfaces_state.v1_0_0.oper_statusEnum">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/enumeration"/>
          <Annotation Term="Redfish.Yang.mandatory" EnumMember="Redfish.Yang.Mandatory/true"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB - ifOperStatus"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The current operational state of the interface. This leaf has the same semantics as ifOperStatus."/>
        </Property>
        <Property Name="last_change" Type="Redfish.Yang.yang:date-and-time">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/yang:date-and-time"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB - ifLastChange"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The time the interface entered its current operational state. If the current state was entered prior to the last re-initialization of the local network management subsystem, then this node is not present."/>
        </Property>
        <Property Name="if_index" Type="Edm.Int32">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/int32"/>
          <Annotation Term="Redfish.Yang.if_feature" String="if-mib"/>
          <Annotation Term="Redfish.Yang.range" String="1..2147483647"/>
          <Annotation Term="Redfish.Yang.mandatory" EnumMember="Redfish.Yang.Mandatory/true"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB - ifIndex"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The ifIndex value for the ifEntry represented by this interface."/>
        </Property>
        <Property Name="phys_address" Type="Redfish.Yang.yang:phys-address">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/yang:phys-address"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB - ifPhysAddress"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The interface's address at its protocol sub-layer. For example, for an 802.x interface, this object normally contains a Media Access Control (MAC) address. The interface's media-specific modules must define the bit and byte ordering and the format of the value of this object. For interfaces that do not have such an address (e.g., a serial line), this node is not present."/>
        </Property>
        <Property Name="higher_layer_if" Type="Collection(Redfish.Yang.interface-state-ref)">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf_list"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/interface-state-ref"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="A list of references to interfaces layered on top of this interface."/>
        </Property>
        <Property Name="lower_layer_if" Type="Collection(Redfish.Yang.interface-state-ref)">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf_list"/>
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/interface-state-ref"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="A list of references to interfaces layered underneath this interface."/>
        </Property>
        <Property Name="speed" Type="Redfish.Yang.yang:gauge64">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf" />
          <Annotation Term="Redfish.Yang.YangType" EnumMember="Redfish.Yang.YangTypes/yang:gauge64" />
          <Annotation Term="Redfish.Yang.units" String="bits/second" />
          <Annotation Term="Redfish.Yang.reference" String="RFC 2863: The Interfaces Group MIB -&#10;                        ifSpeed, ifHighSpeed" />
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="An estimate of the interface's current bandwidth in bits per second. For interfaces that do not vary in bandwidth or for those where no accurate estimation can be made, this node should contain the nominal bandwidth. For interfaces that have no concept of bandwidth, this node is not present." />
        </Property>
        <NavigationProperty Name="statistics" Type="ietf_interfaces.interfaces_state.statistics.statistics" ContainsTarget="true">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="A reference to the statistics resource associated to the interfaces_state."/>
          <Annotation Term="OData.LongDescription" String="The value of this property shall be a reference to statistics associated to the interface_state."/>
          <Annotation Term="OData.AutoExpandReferences"/>
        </NavigationProperty>
      </EntityType>

