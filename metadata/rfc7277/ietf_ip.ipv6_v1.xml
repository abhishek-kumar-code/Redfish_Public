<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/cs01/vocabularies/Org.OData.Core.V1.xml">
    <Include Alias="Odata" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/cs01/vocabularies/Org.OData.Capabilities.V1.xml">
    <Include Alias="Odata" Namespace="Org.OData.Capabilities.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
    <edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
    <edmx:Include Namespace="Validation.v1_0_0" Alias="Validation"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishYangExtensions_v1.xml">
    <edmx:Include Namespace="RedfishYangExtensions.v1_0_0" Alias="Redfish.Yang"/>
    <edmx:Include Namespace="Validation.v1_0_0" Alias="Validation"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/rfc7277/ietf_ip.ipv6.address_v1.xml">
    <edmx:Include Namespace="ietf_ip.ipv6.address"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/rfc7277/ietf_ip.ipv6.neighbor_v1.xml">
    <edmx:Include Namespace="ietf_ip.ipv6.neighbor"/>
  </edmx:Reference>

  <DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6">
      <EntityType Name="ipv6" BaseType="Resource.v1_0_0.Resource" Abstract="true">
        <Annotation Term="OData.Description" String="Parameters for the IPv6 address family."/>
        <Annotation Term="OData.LongDescription" String="Parameters for the IPv6 address family."/>
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record>
            <PropertyValue Property="Insertable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record>
            <PropertyValue Property="Updatable" Bool="true"/>
            <Annotation Term="OData.Description" String="A Chassis can be updated to change some properties such as the IndicatorLED."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record>
            <PropertyValue Property="Deletable" Bool="false"/>
          </Record>
        </Annotation>
      </EntityType>
    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6.v1_0_0">
      <EntityType Name="ipv6" BaseType="ietf_ip.ipv6.ipv6" >
        <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/container"/>
        <Annotation Term="Redfish.Yang.presence" String="Enables IPv6 unless the 'enabled' leaf&#10;           (which defaults to 'true') is set to 'false'" />
        <Annotation Term="OData.Description" String="Parameters for the IPv6 address family." />
        <Property DefaultValue="true" Name="enabled" Type="Edm:Boolean">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf" />
          <Annotation Term="Redfish.Yang.YangType" String="boolean" />
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="Controls whether IPv6 is enabled or disabled on this interface. When IPv6 is enabled, this interface is connected to an IPv6 stack, and the interface can send and receive IPv6 packets." />
        </Property>
        <Property DefaultValue="false" Name="forwarding" Type="Edm:Boolean">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" String="boolean" />
          <Annotation Term="Redfish.Yang.reference" String="RFC 4861: Neighbor Discovery for IP version 6 (IPv6)&#10;                       Section 6.2.1, IsRouter" />
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="Controls IPv6 packet forwarding of datagrams received by, but not addressed to, this interface. IPv6 routers forward datagrams. IPv6 hosts do not (except those source-routed via the host)." />
        </Property>
        <Property Name="mtu" Type="Redfish.Yang.uint32">
          <Annotation Term="RedfishYang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf" />
          <Annotation Term="Redfish.Yang.YangType" String="uint32" />
          <Annotation Term="Redfish.Yang.range" String="1280..max" />
          <Annotation Term="Redfish.Yang.units" String="octets" />
          <Annotation Term="Redfish.Yang.reference" String="RFC 2460: Internet Protocol, Version 6 (IPv6) Specification&#10;                       Section 5" />
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The size, in octets, of the largest IPv6 packet that the interface will send and receive. The server may restrict the allowed values for this leaf, depending on the interface's type. If this leaf is not configured, the operationally used MTU depends on the interface's type." />
        </Property>
        <Property DefaultValue="1" Name="dup_addr_detect_transmits" Type="Redfish.Yang.uint32">
          <Annotation Term="RedfishYang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf" />
          <Annotation Term="Redfish.Yang.YangType" String="uint32"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 4862: IPv6 Stateless Address Autoconfiguration" />
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The number of consecutive Neighbor Solicitation messages sent while performing Duplicate Address Detection on a tentative address. A value of zero indicates that Duplicate Address Detection is not performed on tentative addresses. A value of one indicates a single transmission with no follow-up retransmissions."/>
        </Property>
        <NavigationProperty Name="address" Type="ietf_ip.ipv6.addressCollection.addressCollection">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read/"/>
          <Annotation Term="OData.Description" String="A reference to a collection of ipv6 addresses."/>
          <Annotation Term="OData.LongDescription" String="Shall be a reference to a collection of ipv6 addresses."/>
          <Annotation Term="AutoExpandReferences"/>
        </NavigationProperty>
        <NavigationProperty Name="neighbor" Type="ietf_ip.ipv6.neighborCollection.neighborCollection">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read/"/>
          <Annotation Term="OData.Description" String="A reference to a collection of ipv6 neighbors."/>
          <Annotation Term="OData.LongDescription" String="Shall be a reference to a collection of ipv6 neighbors."/>
          <Annotation Term="AutoExpandReferences"/>
        </NavigationProperty>
        <NavigationProperty Name="autoconf" Type="ietf_ip.ipv6.autoconfCollection.autoconfCollection">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read/"/>
          <Annotation Term="OData.Description" String="A reference to a collection of ipv6 autoconfs."/>
          <Annotation Term="OData.LongDescription" String="Shall be a reference to a collection of ipv6 autoconfs."/>
          <Annotation Term="AutoExpandReferences"/>
        </NavigationProperty>
      </EntityType>
    </Schema>

  </DataServices>
</edmx:Edmx>
