<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/cs01/vocabularies/Org.OData.Core.V1.xml">
    <Include Alias="Odata" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/cs01/vocabularies/Org.OData.Capabilities.V1.xml">
    <Include Alias="Odata" Namespace="Org.OData.Capabilities.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
    <edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
    <edmx:Include Namespace="Validation.v1_0_0" Alias="Validation"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishYangExtensions_v1.xml">
    <edmx:Include Namespace="RedfishYangExtensions.v1_0_0" Alias="Redfish.Yang"/>
    <edmx:Include Namespace="Validation.v1_0_0" Alias="Validation"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/rfc7277/ietf_ip.ipv4.address_v1.xml">
    <edmx:Include Namespace="ietf_ip.ipv4.address"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/rfc7277/ietf_ip.ipv4.neighbor_v1.xml">
    <edmx:Include Namespace="ietf_ip.ipv4.neighbor"/>
  </edmx:Reference>

  <DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv4">
      <EntityType Name="ipv4" BaseType="Resource.v1_0_0.Resource" Abstract="true">
        <Annotation Term="OData.Description" String="Parameters for the IPv4 address family."/>
        <Annotation Term="OData.LongDescription" String="Parameters for the IPv4 address family."/>
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record>
            <PropertyValue Property="Insertable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record>
            <PropertyValue Property="Updatable" Bool="true"/>
            <Annotation Term="OData.Description" String="A Chassis can be updated to change some properties such as the IndicatorLED."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record>
            <PropertyValue Property="Deletable" Bool="false"/>
          </Record>
        </Annotation>
      </EntityType>
    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv4.v1_0_0">
      <EntityType Name="ipv4" BaseType="ietf_ip.ipv4.ipv4">
        <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/container"/>
        <Annotation Term="Redfish.Yang.presence" String="Enables IPv4 unless the 'enabled' leaf&#10;           (which defaults to 'true') is set to 'false'" />
        <Annotation Term="OData.Description" String="Parameters for the IPv4 address family."/>
        <Annotation Term="OData.LongDescription" String="Parameters for the IPv4 address family."/>
        <Property Name="enabled" Type="Edm.Boolean" DefaultValue="true">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" String="boolean"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="Controls whether IPv4 is enabled or disabled on this interface. When IPv4 is enabled, this interface is connected to an IPv4 stack, and the interface can send and receive IPv4 packets."/>
        </Property>
        <Property Name="forwarding" Type="Edm.Boolean" DefaultValue="false">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" String="boolean"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="Controls IPv4 packet forwarding of datagrams received by, but not addressed to, this interface. IPv4 routers forward datagrams. IPv4 hosts do not (except those source-routed via the host)."/>
        </Property>
        <Property Name="mtu" Type="Redfish.Yang.uint16">
          <Annotation Term="Redfish.Yang.NodeType" EnumMember="Redfish.Yang.NodeTypes/leaf"/>
          <Annotation Term="Redfish.Yang.YangType" String="uint16"/>
          <Annotation Term="Redfish.Yang.range" String="68..max"/>
          <Annotation Term="Redfish.Yang.units" String="octets"/>
          <Annotation Term="Redfish.Yang.reference" String="RFC 791: Internet Protocol"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The size, in octets, of the largest IPv4 packet that the interface will send and receive. The server may restrict the allowed values for this leaf, depending on the interface's type. If this leaf is not configured, the operationally used MTU depends on the interface's type."/>
        </Property>
        <NavigationProperty Name="address" Type="ietf_ip.ipv4.addressCollection.addressCollection">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read/"/>
          <Annotation Term="OData.Description" String="A reference to a collection of ipv4 addresses."/>
          <Annotation Term="OData.LongDescription" String="Shall be a reference to a collection of ipv4 addresses."/>
          <Annotation Term="AutoExpandReferences"/>
        </NavigationProperty>
        <NavigationProperty Name="neighbor" Type="ietf_ip.ipv4.addressCollection.neighboodCollection">
          <Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read/"/>
          <Annotation Term="OData.Description" String="A reference to a collection of ipv4 neighbors."/>
          <Annotation Term="OData.LongDescription" String="Shall be a reference to a collection of ipv4 neighbors."/>
          <Annotation Term="AutoExpandReferences"/>
        </NavigationProperty>
      </EntityType>
    </Schema>
  </DataServices>
</edmx:Edmx>
