<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="OData" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Capabilities.V1.xml">
    <edmx:Include Alias="Capabilities" Namespace="Org.OData.Capabilities.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
    <edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishYangExtensions_v1.xml">
    <edmx:Include Namespace="RedfishYangExtensions.v1_0_0" Alias="RedfishYang"/>
  </edmx:Reference>

  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6.autoconf">
      <EntityType Name="autoconf" BaseType="Resource.v1_0_0.Resource" Abstract="true">
        <Annotation Term="OData.Description" String="Parameters to control the autoconfiguration of IPv6 addresses, as described in RFC 4862." />
        <Annotation Term="OData.LongDescription" String="Shall be the parameters to control the autoconfiguration of IPv6 addresses, as described in RFC 4862." />
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record>
            <PropertyValue Property="Insertable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record>
            <PropertyValue Property="Updatable" Bool="true"/>
            <Annotation Term="OData.Description" String="A Chassis can be updated to change some properties such as the IndicatorLED."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record>
            <PropertyValue Property="Deletable" Bool="false"/>
          </Record>
        </Annotation>
      </EntityType>
    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6.autoconf.v1_0_0.autoconf">
      <EntityType Name="autoconf" BaseType="ietf_ip.ipv6.autoconf.autoconf">
        <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/container"/>
        <Annotation Term="RedfishYang.reference" String="RFC 4862: IPv6 Stateless Address Autoconfiguration"/>
        <Annotation Term="OData.Description" String="Parameters to control the autoconfiguration of IPv6 addresses, as described in RFC 4862."/>
        <Annotation Term="OData.LongDescription" String="Shall be the parameters to control the autoconfiguration of IPv6 addresses, as described in RFC 4862."/>
        <Annotation Term="OData.AdditionalProperties" Bool="false"/>
        <Property Name="create_global_addresses" Type="Edm:Boolean" DefaultValue="true">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/boolean"/>
          <Annotation Term="RedfishYang.reference" String="RFC 4862: IPv6 Stateless Address Autoconfiguration&#10;                         Section 5.5"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="If enabled, the host creates global addresses as described in RFC 4862."/>
        </Property>
        <Property Name="create_temporary_addresses" Type="Edm:Boolean" DefaultValue="false">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/boolean"/>
          <Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
          <Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address&#10;                         Autoconfiguration in IPv6"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="If enabled, the host creates temporary addresses as described in RFC 4941."/>
        </Property>
        <Property Name="temporary_valid_lifetime" Type="RedfishYang.uint32" DefaultValue="604800">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/uint32"/>
          <Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
          <Annotation Term="RedfishYang.units" String="seconds"/>
          <Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address&#10;                         Autoconfiguration in IPv6"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The time period during which the temporary address is valid."/>
        </Property>
        <Property Name="temporary_preferred_lifetime" Type="RedfishYang.uint32" DefaultValue="86400">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf" />
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/uint32"/>
          <Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
          <Annotation Term="RedfishYang.units" String="seconds"/>
          <Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address&#10;                         Autoconfiguration in IPv6"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The time period during which the temporary address is preferred."/>
        </Property>
      </EntityType>
    </Schema>

  </edmx:DataServices>
</edmx:Edmx>
