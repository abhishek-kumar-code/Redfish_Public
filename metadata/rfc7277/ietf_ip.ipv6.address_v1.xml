<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="OData" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Capabilities.V1.xml">
    <edmx:Include Alias="Capabilities" Namespace="Org.OData.Capabilities.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
    <edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
  </edmx:Reference>
  <edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishYangExtensions_v1.xml">
    <edmx:Include Namespace="RedfishYangExtensions.v1_0_0" Alias="RedfishYang"/>
  </edmx:Reference>
 
 <edmx:DataServices>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6.address">
      <EntityType Name="address" BaseType="Resource.v1_0_0.Resource" Abstract="true">
        <Annotation Term="OData.Description" String="Parameters for the ipv6 address family."/>
        <Annotation Term="OData.LongDescription" String="Parameters for the ipv6 address family."/>
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record>
            <PropertyValue Property="Insertable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record>
            <PropertyValue Property="Updatable" Bool="true"/>
            <Annotation Term="OData.Description" String="A Chassis can be updated to change some properties such as the IndicatorLED."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record>
            <PropertyValue Property="Deletable" Bool="false"/>
          </Record>
        </Annotation>
      </EntityType>
    </Schema>

    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_ip.ipv6.address.v1_0_0">
      <EntityType Name="address" BaseType="ietf_ip.ipv6.address.address">

        <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/list"/>
        <Annotation Term="RedfishYang.choice" String="subnet"/>
        <Annotation Term="OData.Description" String="The list of configured ipv6 addresses on the interface."/>
        <Property Name="ip" Type="RedfishYangExtensions.v1_0_0.ipv6_address_no_zone">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/ipv6_address_no_zone"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The IPv6 address on the interface."/>
        </Property>
        <Property Name="prefix_length" Type="RedfishYangExtensions.v1_0_0.uint8">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/uint8"/>
          <Annotation Term="RedfishYang.mandatory" EnumMember="RedfishYang.Mandatory/true"/>
          <Annotation Term="RedfishYang.range" String="0..128"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The length of the subnet prefix."/>
        </Property>
        <Property Name="origin" Type="RedfishYangExtensions.v1_0_0.ip_address_origin">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/ip_address_origin"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The origin of this address."/>
        </Property>
        <Property Name="status" Type="ietf_ip.ipv6.address.v1_0_0.statusEnum">
          <Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
          <Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/ip_address_origin"/>
          <Annotation Term="RedfishYang.reference" String="RFC 4293: Management Information Base for the Internet Protocol (IP) - IpAddressStatusTC
RFC 4862: IPv6 Stateless Address Autoconfiguration"/>
          <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
          <Annotation Term="OData.Description" String="The status of an address. Most of the states correspond to states from the IPv6 Stateless Address Autoconfiguration protocol."/>
        </Property>
      </EntityType>

      <EnumType Name="statusEnum">
        <Member Name="preferred">
          <Annotation Term="OData.Description" String="This is a valid address that can appear as the destination or source address of a packet."/>
        </Member>
        <Member Name="deprecated">
          <Annotation Term="OData.Description" String="This is a valid but deprecated address that should no longer be used as a source address in new communications, but packets addressed to such an address are processed as expected."/>
        </Member>
        <Member Name="invalid">
          <Annotation Term="OData.Description" String="This isn’t a valid address, and it shouldn’t appear as the destination or source address of a packet."/>
        </Member>
        <Member Name="inaccessible">
          <Annotation Term="OData.Description" String="The address is not accessible because the interface to which this address is assigned is not operational."/>
        </Member>
        <Member Name="unknown">
          <Annotation Term="OData.Description" String="The status cannot be determined for some reason."/>
        </Member>
        <Member Name="tentative">
          <Annotation Term="OData.Description" String="The uniqueness of the address on the link is being verified. Addresses in this state should not be used for general communication and should only be used to determine the uniqueness of the address."/>
        </Member>
        <Member Name="duplicate">
          <Annotation Term="OData.Description" String="The address has been determined to be non-unique on the link and so must not be used."/>
        </Member>
        <Member Name="optimistic">
          <Annotation Term="OData.Description" String="The address is available for use, subject to restrictions, while its uniqueness on a link is being verified."/>
        </Member>
      </EnumType>
    </Schema>

  </edmx:DataServices>
</edmx:Edmx>
