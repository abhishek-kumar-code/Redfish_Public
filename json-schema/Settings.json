{
    "$schema": "http://schemas.dmtf.org/redfish/v1/redfish-schema.1.0.0.json",
    "title": "Settings",
    "definitions":{
        "Settings":{
            "type": "object",
            "patternProperties": { 
                "^([a-zA-Z_][a-zA-Z0-9_])?@(odata|redfish|Message|Privileges).[a-zA-Z_][a-zA-Z0-9_.]$" : {
                    "type": ["array", "boolean", "number", "null", "object", "string"],
                    "description": "This property shall specify a valid odata or redfish property."
                }
            },
            "additionalProperties": true,
            "properties": {
                "@odata.context": {
                    "$ref": "http://schemas.dmtf.org/redfish/base/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://schemas.dmtf.org/redfish/base/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://schemas.dmtf.org/redfish/base/odata.4.0.0.json#/definitions/type"
                },

            }
        }
    },
