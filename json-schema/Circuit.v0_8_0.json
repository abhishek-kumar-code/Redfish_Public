    "definitions": {
        "Actions": {
            "additionalProperties": false,
            "description": "The available actions for this resource.",
            "longDescription": "This type shall contain the available actions for this resource.",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
                    "type": [
                        "array",
                        "boolean",
                        "integer",
                        "number",
                        "null",
                        "object",
                        "string"
                    ]
                }
            },
            "properties": {
                "#Circuit.PowerControl": {
                    "$ref": "#/definitions/PowerControl"
                },
                "#Circuit.ResetBreaker": {
                    "$ref": "#/definitions/ResetBreaker"
                },
                "Oem": {
                    "$ref": "#/definitions/OemActions",
                    "description": "This property contains the available OEM specific actions for this resource.",
                    "longDescription": "This property shall contain any additional OEM actions for this resource."
                }
            },
            "type": "object"
        },
        "BreakerStates": {
            "enum": [
                "Normal",
                "Tripped",
                "Off",
                "Reset"
            ],
            "enumDescriptions": {
                "Normal": "The breaker is powered on.",
                "Off": "The breaker is off.",
                "Reset": "The breaker is to be reset to the Normal state.",
                "Tripped": "The breaker has been tripped."
            },
            "type": "string"
        },
        "Circuit": {
            "additionalProperties": false,
            "description": "This is the schema definition for an electrical circuit.",
            "longDescription": "This resource shall be used to represent an electrical circuit for a Redfish implementation.",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
                    "type": [
                        "array",
                        "boolean",
                        "integer",
                        "number",
                        "null",
                        "object",
                        "string"
                    ]
                }
            },
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_3.json#/definitions/context"
                },
                "@odata.etag": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_3.json#/definitions/etag"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_3.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_3.json#/definitions/type"
                },
                "Actions": {
                    "$ref": "#/definitions/Actions",
                    "description": "The available actions for this resource.",
                    "longDescription": "The Actions property shall contain the available actions for this resource."
                },
                "BreakerState": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/BreakerStates"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": ".",
                    "longDescription": ".",
                    "readonly": false
                },
                "CircuitType": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/CircuitType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The type of circuit.",
                    "longDescription": ".",
                    "readonly": true
                },
                "Current": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The current sensor for this circuit.",
                    "longDescription": "This object shall contain the current, measured in Amperes, for this single phase circuit. This object shall not appear in resource instances representing poly-phase circuits.",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "readonly": true
                },
                "Energy": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The energy sensor for this circuit.",
                    "longDescription": "This object shall contain the total energy, measured in Watt-hours (units of Watts), for this circuit, which represents the 'Total' ElectricalSourceType sensor when multiple energy sensors exist for this circuit.",
                    "readonly": true
                },
                "Frequency": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The frequency sensor for this circuit.",
                    "longDescription": "TBD.",
                    "readonly": true
                },
                "IECOutletType": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/IECType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The IEC outlet type.",
                    "longDescription": ".",
                    "readonly": true
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "IndicatorLED": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/IndicatorLED"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The state of the indicator LED, used to identify the circuit.",
                    "longDescription": "The value of this property shall contain the indicator light state for the indicator light associated with this circuit.",
                    "readonly": false
                },
                "LockingOutlet": {
                    "description": "Designates if the outlet is a locking type.",
                    "longDescription": "The value of this property shall indicate whether the outlet includes a locking design. The value shall be true if the outlet is a locking type, and false if the outlet is a non-locking type.",
                    "readonly": true,
                    "type": [
                        "boolean",
                        "null"
                    ]
                },
                "NEMAOutletType": {
                    "description": "The NEMA outlet type.",
                    "longDescription": ".",
                    "readonly": true,
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "NominalVoltage": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/NominalVoltageType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The nominal voltage for this circuit.",
                    "longDescription": "This property shall represent the nominal voltage for this circuit, in Volts.",
                    "readonly": true
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "PhaseWiringType": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/PhaseWiringType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The phase wiring of the circuit.",
                    "longDescription": ".",
                    "readonly": true
                },
                "PolyPhaseCurrents": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Currents"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The current sensors for this circuit.",
                    "longDescription": "TBD."
                },
                "PolyPhasePowerReadings": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/PowerReadings"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The power sensors for this circuit.",
                    "longDescription": "TBD."
                },
                "PolyPhaseVoltages": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Voltages"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The voltage sensors for this circuit.",
                    "longDescription": "This object shall contain the voltage sensor(s) for this circuit. For single phase circuits this object will contain a duplicate copy of the voltage sensor referenced in the 'Voltage' object. For poly-phase circuits this object may contain multiple voltage sensor readings used to fully describe the circuit."
                },
                "PowerEnabled": {
                    "description": "Indicates if the circuit can be powered.",
                    "longDescription": "The value of this property shall contain the power enable state of the circuit.  True shall indicate that the circuit may be powered on, and false shall indicate that the circuit cannot be powered.",
                    "readonly": true,
                    "type": [
                        "boolean",
                        "null"
                    ]
                },
                "PowerOffDelaySeconds": {
                    "description": "The number of seconds to delay power off after a PowerControl request. Zero seconds indicates no delay to power off.",
                    "longDescription": "TBD.",
                    "readonly": false,
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "PowerOnDelaySeconds": {
                    "description": "The number of seconds to delay power up after a power cycle or a PowerControl request. Zero seconds indicates no delay to power up.",
                    "longDescription": "TBD.",
                    "readonly": false,
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "PowerReading": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The power sensor for this circuit.",
                    "longDescription": "This object shall contain the total power, measured in Watts, for this circuit, which represents the 'Total' ElectricalSourceType sensor when multiple power sensors exist for this circuit.",
                    "readonly": true
                },
                "PowerState": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/PowerState"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The power state of the circuit.",
                    "longDescription": "The value of this property shall contain the power state of the circuit.",
                    "readonly": true
                },
                "RatedCurrentAmps": {
                    "description": "The rated maximum current allowed for this circuit.",
                    "longDescription": "This property shall represent the rated maximum current for this circuit, in Amps.",
                    "minimum": 0,
                    "readonly": true,
                    "type": [
                        "number",
                        "null"
                    ],
                    "units": "A"
                },
                "Status": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Status",
                    "description": "This property describes the status and health of the resource and its children.",
                    "longDescription": "This property shall contain any status or health properties of the resource."
                },
                "Temperature": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": ".",
                    "longDescription": ".",
                    "readonly": true
                },
                "Voltage": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Sensor.json#/definitions/Sensor"
                        },
                        {
                            "type": "null"
                        }
