                "VoltageType": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/VoltageType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The type of voltage output on the circuit.",
                    "longDescription": ".",
                    "readonly": true
                }
            },
            "required": [
                "@odata.id",
                "@odata.type",
                "Id",
                "Name"
            ],
            "type": "object"
        },
        "CircuitType": {
            "enum": [
                "Mains",
                "Branch",
                "Outlet"
            ],
            "enumDescriptions": {
                "Branch": "A branch (output) circuit.",
                "Mains": "A Mains input circuit.",
                "Outlet": "A power outlet (receptacle)."
            },
            "type": "string"
        },
