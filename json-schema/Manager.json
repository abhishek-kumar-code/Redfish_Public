{
    "$schema": "http://schemas.dmtf.org/redfish/v1/redfish-schema.1.0.0.json",
    "title": "Manager",
    "$ref": "#/definitions/Manager",
    "definitions":{
        "ForceFailover":{
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "description": "Link to invoke action"
                },
                "href@Redfish.AllowableValues": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "@odata.id": "http://schemas.dmtf.org/redfish/v1/Resource.json#Resource.Resource"
                        }
                    },
                    "description": "This array defines the supported values for this property on this resource.  In this case, a valid reference is supported."
                }
            }

        },
        "Manager":{
                "oneOf": [ 
                    {
                        "$ref": "http://schemas.dmtf.org/redfish/v1/odata.4.0.0.json#/definitions/idRef"
                    },
                    {
                        "$ref": "http://schemas.dmtf.org/redfish/Manager.1.0.0.json#/definitions/Manager"
                    }
                ]
            }
        }
        },
        "Reset":{
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "description": "Link to invoke action"
                },
                "ResetType@Redfish.AllowableValues": {
                    "type": "array",
                    "items": {
                        "$ref": "http://schemas.dmtf.org/redfish/v1/Resource.json#/definitions/ResetType"
                    },
                    "description": "This is the type of reset to be performed.",
                    "longDescription": "This defines the property name for the action."
                }
            }

        },
        "ModifyRedundancySet":{
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "description": "Link to invoke action"
                },
                "Add@Redfish.AllowableValues": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "@odata.id": "http://schemas.dmtf.org/redfish/v1/Resource.json#Resource.Resource"
                        }
                    },
                    "description": "This array defines the supported values for this property on this resource.  In this case, a valid reference is supported. Add shall be used to Add members to the manager redundancy group."
                },
                "Remove@Redfish.AllowableValues": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "@odata.id": "http://schemas.dmtf.org/redfish/v1/Resource.json#Resource.Resource"
                        }
                    },
                    "description": "This array defines the supported values for this property on this resource.  In this case, a valid reference is supported. Remove shall be used to remove members from the manager redundancy group."
                }
            }

        }
    },
