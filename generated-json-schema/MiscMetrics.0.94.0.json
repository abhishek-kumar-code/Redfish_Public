{
    "$schema": "http://www.redfishspecification.org/schema",
        "MiscMetrics":{
            "title": "MiscMetrics.0.94.0",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/odataContext"
                },
                "@odata.id": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/odataId"
                },
                "@odata.type": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/odataType"
                },
                "Id": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Id"
                },
                "Description": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Description"
                },
                "Name": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Name"
                },
                "Modified": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Modified"
                },
                "Oem": {
                    "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Intrusion": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Name": {
                                "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Name"
                            },
                            "Number": {
                                "type": [
                                    "string",
                                    "null"
                                ],
                                "readonly": true,
                                "description": "The sensor's number, if available.",
                                "longDescription": "The value of this property shall be the EntityInstance from IPMI spec if sensor is consistent with IPMI's SEL, otherwise it is implementation specific."
                            },
                            "Status": {
                                "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Status"
                            },
                            "CurrentReading": {
                                "type": [
                                    "string",
                                    "null"
                                ],
                                "readonly": true,
                                "description": "Current value of the metric or sensor.",
                                "longDescription": "The value of this property shall be the current reading of the metric or sensor."
                            },
                            "SensorType": {
                                "type": "string",
                                "enum": [
                                    "Physical(Chassis) Security",
                                    "Platform Security Violation Attempt"
                                ],
                                "readonly": true,
                                "description": "The type of sensor",
                                "longDescription": "This property shall contain the enumerated type of Intrusion sensor described by this instance."
                            }
                        }
                    },
                    "readonly": true
                },
                "Misc": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Name": {
                                "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Name"
                            },
                            "Number": {
                                "type": [
                                    "string",
                                    "null"
                                ],
                                "readonly": true,
                                "description": "The sensor's number, if available.",
                                "longDescription": "The value of this property shall be the EntityInstance from IPMI spec if sensor is consistent with IPMI's SEL, otherwise it is implementation specific."
                            },
                            "Status": {
                                "$ref": "http://localhost:9080/rest/v1/dmtf.org/schema/v1/Resource#Status"
                            },
                            "CurrentReading": {
                                "type": [
                                    "string",
                                    "null"
                                ],
                                "readonly": true,
                                "description": "Current value of the metric or sensor.",
                                "longDescription": "The value of this property shall be the current reading of the metric or sensor."
                            },
                            "SensorType": {
                                "type": "string",
                                "enum": [
                                    "OtherUnitsBasedSensor",
                                    "Memory",
                                    "DriveSlotOrBay",
                                    "POSTMemoryResize",
                                    "SystemFirmwareProgress",
                                    "EventLoggingDisabled",
                                    "SystemEvent",
                                    "CriticalInterrupt",
                                    "ButtonOrSwitch",
                                    "ModuleOrBoard",
                                    "MicrocontrollerOrCoprocessor",
                                    "AddInCard",
                                    "Chassis",
                                    "ChipSet",
                                    "OtherFRU",
                                    "CableOrInterconnect",
                                    "Terminator",
                                    "SystemBootOrRestart",
                                    "BootError",
                                    "BaseOSBootOrInstallationStatus",
                                    "OSStopOrShutdown",
                                    "SlotOrConnector",
                                    "SystemACPIPowerState",
                                    "Watchdog",
                                    "PlatformAlert",
                                    "EntityPresence",
                                    "MonitorASICOrIC",
                                    "LAN",
                                    "ManagementSubsystemHealth",
                                    "SessionAudit",
                                    "VersionChange",
                                    "FRUState",
                                    "Humidity",
                                    "Water"
                                ],
                                "readonly": true,
                                "description": "The type of sensor",
                                "longDescription": "This property shall contain the enumerated type of Misc sensor described by this instance"
                            }
                        }
                    },
                    "readonly": true
                }
            },
            "description": "This is the schema definition for the Miscellaneous Metrics resource.  It represents any sensors or metrics that are not thermal or power related.  Examples of these are intrusion detection and various alarms.",
            "longDescription": "This resource shall be used to represent the miscellaneous metrics resources for an SPMA implementation."        
}
}

